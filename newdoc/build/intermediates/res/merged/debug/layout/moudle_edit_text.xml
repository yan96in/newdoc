<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/relativeLayout_title"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@color/bg_def">

    <TextView
        android:id="@+id/line0"
        android:layout_width="match_parent"
        android:layout_height="0.5dip"
        android:background="@color/stoke_dark" />

    <EditText
        android:id="@+id/met_et"
        style="@style/Text.Large1.darkgray"
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:layout_below="@+id/line0"
        android:background="@color/white"
        android:drawableLeft="@drawable/ic_id"
        android:drawablePadding="15dip"
        android:hint=""
        android:paddingLeft="18dip" />

    <TextView
        android:id="@+id/line1"
        android:layout_width="match_parent"
        android:layout_height="0.5dip"
        android:layout_below="@+id/met_et"
        android:background="@color/stoke_dark" />

</RelativeLayout><!-- From: file:/C:/Users/Jiancheng/Desktop/newdoc9/newdoc/src/main/res/layout/moudle_edit_text.xml -->